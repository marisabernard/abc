  "scripts": {
    "lint": "tslint \"**/*.ts?(x)\" --exclude \"**/node_modules/**\" --exclude \"More/**\" --exclude \"*SKIP*/**\"",
    "lint:fix": "tslint --fix \"**/*.ts?(x)\" --exclude \"**/node_modules/**\" --exclude \"More/**\"",
    "fixdb": "copyfiles -f demo-data/backup/db.json demo-data",
    "live": "live-server --no-browser --no-css-inject --wait=200 --proxy=/api:http://localhost:8085 -q",
    "json": "json-server --port 8085 --host 0.0.0.0 --delay 200 --watch demo-data/db.json",
    "start": "concurrently --kill-others \"npm run live\" \"npm run json\""
  }
